<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pagemodels="clr-namespace:JwtAuth.MAUI.PageModels"
             x:Class="JwtAuth.MAUI.Pages.ChangeUserPasswordPage"
             x:DataType="{x:Type pagemodels:ChangeUserPasswordPageModel}"
             Title="Change User Password">
    <VerticalStackLayout>
        <Label Text="Password:" />
        <!--<Entry Text="{Binding RegisterUser.Password, Mode=OneWayToSource}"
    IsPassword="True" Placeholder="password" MaxLength="50" />-->
        <Entry Text="Password" IsPassword="True" Placeholder="password" MaxLength="50" />
        <!--<Button Command="{Binding LoginCommand, Mode=OneWay}" Text="Login" Margin="0,0,0,40" />-->
        <HorizontalStackLayout>
            <Button Text="Save" Margin="0,0,0,40" />
            <Button Text="Cancel" />
        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentPage>
