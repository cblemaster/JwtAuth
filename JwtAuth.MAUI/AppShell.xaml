<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="JwtAuth.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:JwtAuth.MAUI.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="JwtAuth">
    <!--xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"-->
    <TabBar>
        <ShellContent Title="Login" ContentTemplate="{DataTemplate pages:LoginPage}"
                      Route="Login" />
        <ShellContent Title="Logout" ContentTemplate="{DataTemplate pages:LogoutPage}"
                      Route="Logout" />
    </TabBar>
</Shell>
