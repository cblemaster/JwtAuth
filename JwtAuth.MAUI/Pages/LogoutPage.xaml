<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pagemodels="clr-namespace:JwtAuth.MAUI.PageModels"
             x:Class="JwtAuth.MAUI.Pages.LogoutPage"
             x:DataType="{x:Type pagemodels:LogoutPageModel}"
             Title="Logout">
    <VerticalStackLayout HorizontalOptions="Center">
        <Button Command="{Binding LogoutCommand, Mode=OneWay}" Text="Logout" Margin="0,0,0,40" />
    </VerticalStackLayout>
</ContentPage>
